function bool=compatible(A,B)

bool=all(size(A.m)==size(B.m))&&...
     all(size(A.n)==size(B.n))&&...
     all(size(A.c)==size(B.c))&&...
     all(size(A.s)==size(B.s))&&...
     all(size(A.m)==size(A.n))&&...
     all(size(A.n)==size(B.c))&&...
     all(size(A.c)==size(B.s))&&...
     A.m0ind==B.m0ind &&...
     A.n0ind==B.n0ind;
     
if bool
 bool=isnan(A.c(A.m0ind,end))==isnan(B.c(B.m0ind,end));
end
     

  
  
 